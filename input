sklenice1 = input("Zadejte název nápoje = ")
sklenice2 = input("Zadejte název nápoje = ")

print(f"sklenice 1 = {sklenice1}")
print(f"sklenice 2 = {sklenice2}")

print("Přidáme sklenici 3")

sklenice3 = ""

print("Přelejeme sklenici 1 do sklenice 3")

sklenice3 = sklenice1
sklenice1 = ""

print(f"sklenice 3 = ", sklenice3)
print(f"sklenice 1 = ", sklenice1)